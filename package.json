{
  "name": "rewrite",
  "version": "0.0.1",
  "description": "Vue3 和 React 组件互转",
  "files": [
    "dist"
  ],
  "main": "./dist/rewrite.umd.js",
  "module": "./dist/rewrite.esm.js",
  "exports": {
    ".": {
      "import": "./dist/rewrite.esm.js",
      "require": "./dist/rewrite.umd.js"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -c rollup.config.js",
    "dev-watch": "cross-env NODE_ENV=development rollup -c rollup.config.js -w",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build-watch": "cross-env NODE_ENV=production rollup -c rollup.config.js -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UltramanWeiLai/rewrite.git"
  },
  "author": "weilai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UltramanWeiLai/rewrite/issues"
  },
  "homepage": "https://github.com/UltramanWeiLai/rewrite#readme",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime-corejs3": "^7.18.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "cross-env": "^7.0.3",
    "rollup": "^2.75.7",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vue": "^3.2.37"
  }
